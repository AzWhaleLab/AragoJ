<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<!--prefWidth="100" maxWidth="Infinity" minWidth="-Infinity"-->
<!--style="-fx-background-color: #FFFFFF; -fx-border-color: #C2C2C2; -fx-border-width: 1;"-->

<HBox fx:id="hBox" alignment="CENTER_LEFT" style="-fx-background-color: null;" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1">
    <Label fx:id="image" style="-fx-background-color: #FFFFFF;">
        <HBox.margin>
            <Insets right="5.0" />
        </HBox.margin>
    </Label>
    <VBox alignment="CENTER_LEFT" style="-fx-background-color: null;" HBox.hgrow="NEVER">
        <children>
            <Label fx:id="primaryLabel" text="Label" />
            <TextField id="cellRenamableTextField" fx:id="primaryTextField" text="Label" />
            <Label id="cellSecondaryField" fx:id="secondaryLabel" maxWidth="Infinity" minWidth="-Infinity" text="Label" textFill="#ababab" />
        </children>
        <HBox.margin>
            <Insets bottom="5.0" right="5.0" top="5.0" />
        </HBox.margin>
    </VBox>
</HBox>
